page{
  width: 100%;
  height: 100%;
  background: #595B61;

}
.musicwrap{
  width: 100%;
  height: 300px;
  /* background: red; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.circle1{
  width: 250px;
  height: 250px;
  border-radius: 50%;
  background: #606369;
  display: flex;
  justify-content: center;
  align-items: center;
  
}
.circle2{
  width: 220px;
  height: 220px;
  border-radius: 50%;
  background: #2E2E2E;
  display: flex;
  justify-content: center;
  align-items: center;
}
.circle3{
  width: 150px;
  height: 150px;
  border-radius: 50%;
  overflow: hidden;
}
.circle3 image{
  width: 100%;
  height: 100%;
}
.musicContorl{
  display: flex;
  justify-content: space-around;
}
.musicContorl image{
  width: 60px;
  height: 60px;
}